<?xml version='1.0'?><!-- Game is either sse, tesv, or fo4 -->
<PapyrusProject xmlns="PapyrusProject.xsd" 
    Flags="TESV_Papyrus_Flags.flg" 
    Game="sse" 
    Output="..\..\Scripts" 
    Optimize="true"
    Anonymize="true" 
    Release="false" 
    Final="false">
    <Variables>
        <!-- The folder this mod is located in. You should change this! -->
        <Variable Name="modname" Value="CustomSkillsMenuFixed"/>
        <Variable Name="modpath" Value="."/>
        <!-- Path to Skyrim installation -->
        <Variable Name="gamepath" Value="E:\SteamLibrary\steamapps\common\Skyrim Special Edition"/>
    </Variables>
    <Imports>
    
        <Import>.</Import>
        <Import>https://github.com/eeveelo/PapyrusUtil/tree/master/Scripts/Source</Import>
        <Import>https://github.com/MrOctopus/nl_util/tree/main/dist/scripts/source</Import>
        <Import>https://github.com/MrOctopus/nl_online/tree/main/skyui/source</Import>
        <Import>https://github.com/MrOctopus/nl_online/tree/main/skse64/source</Import>
        <Import>https://github.com/MrOctopus/nl_online/tree/main/sse/source</Import>
        <Import>https://github.com/MrOctopus/nl_online/tree/main/jcontainersse/source</Import>
        <Import>https://github.com/clayne/ConsoleUtilSSE/tree/master/src/papyrus</Import>
        <Import>https://github.com/Exit-9B/MCM-Helper/tree/main/scripts/private</Import>
        <Import>https://github.com/Exit-9B/MCM-Helper/tree/main/scripts/public</Import>
        <Import>https://github.com/Exit-9B/CustomSkills/tree/main/scripts</Import>
        <Import>https://github.com/powerof3/PapyrusExtenderSSE/tree/master/Papyrus/Source/scripts</Import>
        <Import>@gamepath\Data\Scripts\Source</Import>
    </Imports>
    <Folders>
        <Folder>.</Folder>
    </Folders>
</PapyrusProject>